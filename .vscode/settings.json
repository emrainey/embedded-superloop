        "/Applications/ArmGNUToolchain/12.3.rel1/arm-none-eabi/lib/gcc/arm-none-eabi/12.3.1/include",
        "/Applications/ArmGNUToolchain/12.3.rel1/arm-none-eabi/lib/gcc/arm-none-eabi/12.3.1/include-fixed",
        "/Applications/ArmGNUToolchain/12.3.rel1/arm-none-eabi/lib/gcc/arm-none-eabi/12.3.1/../../../../arm-none-eabi/include",
        "${workspaceFolder}/modules/jarnax/include",
        "${workspaceFolder}/modules/core/include",
        "${workspaceFolder}/modules/memory/include",
        "${workspaceFolder}/modules/cortex/include",
        "${workspaceFolder}/modules/stm32/include",
        "${workspaceFolder}/applications/unittests"
    ],
    // "C_Cpp.default.compilerPath": "/Applications/ARM/bin/arm-none-eabi-g++",
    "C_Cpp.default.compilerPath": "/Applications/ArmGnuToolchain/12.3.pre1/bin/arm-none-eabi-g++",
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "C_Cpp.codeAnalysis.clangTidy.useBuildPath": true,
    "C_Cpp.intelliSenseEngineFallback": "enabled",
    "C_Cpp.loggingLevel": "Debug",
    "C_Cpp.codeAnalysis.clangTidy.checks.disabled": [
        "clang-diagnostic-deprecated-register",
        "readability-convert-member-functions-to-static"
    ],
    "clang-format.style": "file",
    "clang.completion.enable": true,
    "clang.diagnostic.delay": 500,
    "clang.diagnostic.enable": true,
    "clang.executable": "/usr/bin/clang",
    "clang.cxxflags": [
        "-std=c++14",
        "-fno-exceptions",
        "-fno-rtti",
        "-fno-threadsafe-statics",
        "-fno-use-cxa-atexit"
    ],
    "clang.cflags": [
        "-std=c17",
    ],
    "testMate.cpp.test.advancedExecutables": [
        {
            "pattern": "build/**/test-*",
            "cwd": "${workspaceFolder}",
        }
    ],
    "cmake.allowCommentsInPresetsFile": false,
    "cmake.allowUnsupportedPresetsVersions": true,
    "cmake.buildEnvironment": {
        "CMAKE_VERBOSE": "1"
    },
    "cmake.configureArgs": [
        "--graphviz=jarnax.dot"
    ],
    "cmake.deleteBuildDirOnCleanConfigure": true
